		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		
		<supported-publishing-event>
			<qname xmlns:publishName="http:eventNavigaNatura/events">publishName:event.navigaNatura</qname>
		</supported-publishing-event>
		 
	</portlet>
	
	<portlet id="natura-portlet3">
		<portlet-name>natura-portlet3</portlet-name>
